<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="CGG\ConferenceBundle\Entity\MenuItemRepository" name="CGG\ConferenceBundle\Entity\MenuItem">
    <id name="id" type="integer" column="menuItem_id">
      <generator strategy="AUTO"/>
    </id>
    <field name="title" type="string" column="menuItem_title" length="255"/>
    <field name="depth" type="integer" column="menuItem_depth"/>
  </entity>
</doctrine-mapping>
