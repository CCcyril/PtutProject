<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="CGG\ConferenceBundle\Entity\Role">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="name" type="string" column="role_name" length="255"/>
    <field name="role" type="string" column="role_role" length="255"/>
      <!--TODO : ajouter join table, join column... pour tenter de fixer l'erreur does not contain the required 'inversedBy-->
    <many-to-many target-entity="User" field="users" mapped-by="roles" />
  </entity>
</doctrine-mapping>
