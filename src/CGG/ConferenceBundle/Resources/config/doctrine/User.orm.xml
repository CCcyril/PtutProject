<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity repository-class="CGG\ConferenceBundle\Entity\UserRepository" name="CGG\ConferenceBundle\Entity\User">
    <id name="id" type="integer" column="id">
      <generator strategy="AUTO"/>
    </id>
    <field name="lname" type="string" column="lname" length="255"/>
    <field name="fname" type="string" column="fname" length="255"/>
    <field name="email" type="string" column="email" length="255"/>
    <field name="address" type="string" column="address" length="255"/>
    <field name="country" type="string" column="country" length="255"/>
    <field name="zipcode" type="string" column="zipcode" length="255"/>
    <field name="status" type="string" column="status" length="255"/>
    <field name="phone" type="string" column="phone" length="255"/>
    <field name="inscriptionDate" type="string" column="inscription_date" length="255"/>
  </entity>
</doctrine-mapping>
